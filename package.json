{
  "name": "patient-doctor-connection",
  "version": "1.0.0",
  "description": "Simple server to connect patients with doctors",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "vite build",
    "preview": "vite preview",
    "dev:client": "vite",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:client\"",
    "tunnel": "ngrok http 3000",
    "tunnel:auth": "ngrok config add-authtoken YOUR_AUTHTOKEN_HERE",
    "dev:tunnel": "concurrently \"npm run dev\" \"npm run tunnel\""
  },
  "dependencies": {
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "firebase": "^12.3.0",
    "firebase-admin": "^13.5.0",
    "mongoose": "^8.18.1",
    "ngrok": "^5.0.0-beta.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^7.6.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "vite": "^4.4.5"
  },
  "keywords": [
    "patient",
    "doctor",
    "websocket",
    "real-time",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "type": "commonjs"
}
